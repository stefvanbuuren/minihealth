{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://groeidiagrammen.nl/schema/EPremDossier.json",
  "type": "object",
  "title": "EPremDossier",
  "required": [
    "Referentie",
    "OrganisatieCode",
    "ClientGegevens",
    "Contactmomenten"
  ],
  "properties": {
    "Referentie": {
      "$id": "#/properties/Referentie",
      "type": "string",
      "title": "Request identification",
      "default": "",
      "examples": [
        "fa308134-069e-49ce-9847-ccdae380ed6f"
      ],
      "pattern": "^(.*)$"
    },
    "OrganisatieCode": {
      "$id": "#/properties/OrganisatieCode",
      "type": "integer",
      "title": "Care organisation code",
      "default": 0,
      "examples": [
        1234
      ]
    },
    "ClientGegevens": {
      "$id": "#/properties/ClientGegevens",
      "type": "object",
      "title": "Child data object",
      "required": [
        "Elementen",
        "Groepen"
      ],
      "properties": {
        "Elementen": {
          "$id": "#/properties/ClientGegevens/properties/Elementen",
          "type": "array",
          "title": "The Elementen Schema",
          "items": {
            "$id": "#/properties/ClientGegevens/properties/Elementen/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "Bdsnummer",
              "Waarde"
            ],
            "properties": {
              "Bdsnummer": {
                "$id": "#/properties/
                Gegevens/properties/Elementen/items/properties/Bdsnummer",
                "type": "integer",
                "title": "The Bdsnummer Schema",
                "default": 0,
                "examples": [
                  19
                ]
              },
              "Waarde": {
                "$id": "#/properties/ClientGegevens/properties/Elementen/items/properties/Waarde",
                "type": "string",
                "title": "The Waarde Schema",
                "default": "",
                "examples": [
                  "2"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "Groepen": {
          "$id": "#/properties/ClientGegevens/properties/Groepen",
          "type": "array",
          "title": "The Groepen Schema",
          "items": {
            "$id": "#/properties/ClientGegevens/properties/Groepen/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "Elementen"
            ],
            "properties": {
              "Elementen": {
                "$id": "#/properties/ClientGegevens/properties/Groepen/items/properties/Elementen",
                "type": "array",
                "title": "The Elementen Schema",
                "items": {
                  "$id": "#/properties/ClientGegevens/properties/Groepen/items/properties/Elementen/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "Bdsnummer",
                    "Waarde"
                  ],
                  "properties": {
                    "Bdsnummer": {
                      "$id": "#/properties/ClientGegevens/properties/Groepen/items/properties/Elementen/items/properties/Bdsnummer",
                      "type": "integer",
                      "title": "The Bdsnummer Schema",
                      "default": 0,
                      "examples": [
                        63
                      ]
                    },
                    "Waarde": {
                      "$id": "#/properties/ClientGegevens/properties/Groepen/items/properties/Elementen/items/properties/Waarde",
                      "type": "string",
                      "title": "The Waarde Schema",
                      "default": "",
                      "examples": [
                        "19950704"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "Contactmomenten": {
      "$id": "#/properties/Contactmomenten",
      "type": "array",
      "title": "Child data per visit",
      "items": {
        "$id": "#/properties/Contactmomenten/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "Tijdstip",
          "Elementen"
        ],
        "properties": {
          "Tijdstip": {
            "$id": "#/properties/Contactmomenten/items/properties/Tijdstip",
            "type": "string",
            "title": "The Tijdstip Schema",
            "default": "",
            "examples": [
              "20181111"
            ],
            "pattern": "^(.*)$"
          },
          "Elementen": {
            "$id": "#/properties/Contactmomenten/items/properties/Elementen",
            "type": "array",
            "title": "The Elementen Schema",
            "items": {
              "$id": "#/properties/Contactmomenten/items/properties/Elementen/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "Bdsnummer",
                "Waarde"
              ],
              "properties": {
                "Bdsnummer": {
                  "$id": "#/properties/Contactmomenten/items/properties/Elementen/items/properties/Bdsnummer",
                  "type": "integer",
                  "title": "The Bdsnummer Schema",
                  "default": 0,
                  "examples": [
                    235
                  ]
                },
                "Waarde": {
                  "$id": "#/properties/Contactmomenten/items/properties/Elementen/items/properties/Waarde",
                  "type": "string",
                  "title": "The Waarde Schema",
                  "default": "",
                  "examples": [
                    "380"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }
}